<ul>
  <% if user_signed_in? %>
    <h2>Welcome <%= current_user.first_name %> <%=current_user.last_name %>!!</h2>
    <%= button_to "Sign Out", destroy_user_session_path, method: :delete %><br>
  <% else %>
    <%= button_to "Sign Up", new_user_registration_path, method: :get %>
    <%= button_to "Sign In", new_user_session_path, method: :get %>
  <% end %>
  <%=yield%>
</ul>
